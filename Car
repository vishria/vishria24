public class Car
{
 String brand;
 int year;
 void displayInfo()
{
 System.out.println("Brand: " + brand);
 System.out.println("Year: " + year);
}
 void startEngine()
{
 System.out.println(brand + " engine started!");
}
public static void main(String[] args)
{
 Car myCar = new Car();
 myCar.brand = "Toyota";
 myCar.year = 2022;
 myCar.displayInfo();
 myCar.startEngine();
 }
}
